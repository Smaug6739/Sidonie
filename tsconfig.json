{
	"version": "1.0.0",
	"compilerOptions": {
		"lib": [
			"es6"
		],
		"target": "es6",
		"module": "commonjs",
		"noEmitOnError": true,
		"strict": true,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": false,
		"outDir": "build",
		"rootDir": "./src",
		"removeComments": false,
		"noImplicitAny": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
	},
	"exclude": [
		"node_modules"
	]
}